{
	"extends": ["prettier", "airbnb-base"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"env": {
		"commonjs": true,
		"node": true
	},
	"rules": {
		"prettier/prettier": 0,
		"no-console": 0,
		"linebreak-style": 0
	},
	"plugins": ["prettier"]
}

